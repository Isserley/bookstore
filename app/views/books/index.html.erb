<p id="notice"><%= notice %></p>

<h1>Listing Books</h1>

<table>

  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @books.each do |book| %>
      <p><%= book.title %><br>
      <%= book.author %><br>
      <%= link_to 'Show', book_path(book.id) %>
    <% end %>
  </tbody>


</table>
